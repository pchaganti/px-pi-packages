{
  "name": "pi-packages",
  "version": "1.0.0",
  "private": true,
  "description": "Extensions for pi, the AI coding agent",
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "npm run lint && npm run typecheck",
    "check:ci": "biome ci . && tsc -p tsconfig.json --noEmit",
    "test": "vitest run"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "pi",
    "pi-package",
    "pi-packages",
    "pi-extensions",
    "pi-coding-agent",
    "extensions",
    "ai",
    "ai-provider",
    "coding-assistant",
    "synthetic"
  ],
  "pi": {
    "extensions": [
      "./packages/pi-synthetic-provider/extensions/index.ts",
      "./packages/pi-antigravity-image-gen/extensions/index.ts",
      "./packages/pi-exa-mcp/extensions/index.ts",
      "./packages/pi-firecrawl-mcp/extensions/index.ts",
      "./packages/pi-ancestor-discovery/extensions/index.ts"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@mariozechner/pi-ai": "*",
    "@mariozechner/pi-coding-agent": "*",
    "@sinclair/typebox": "^0.34.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.18"
  },
  "license": "MIT"
}
